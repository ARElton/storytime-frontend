time.title.toLowerCase().includes(query.toLowerCase())\n  })\n\nconst displayedActivities = activities \n  .filter((activity) => {\n    return activity.title.toLowerCase().includes(query.toLowerCase())\n  })\n\n//--------------Login/Logout--------------//\n\n//---------FOR DEVELOPMENT---------//\n\n  // auto-login\n  useEffect(() => {\n    fetch(\"http://localhost:3000/autologin\")\n      .then((r) => r.json())\n      .then(setCurrentUser);\n  }, []);\n\n//---------FOR USE---------//\n\n// Manual Login\nfunction handleLogin() {\n  fetch(\"http://localhost:3000/autologin\")\n    .then((r) => r.json())\n    .then(setCurrentUser)\n}\n\n// Manual Logout\nfunction handleLogout() {\n  setCurrentUser(null)\n}\n\n//--------------Return--------------//\n\n  return (\n    <div className=\"big-container\">\n      <Header \n        query = {query} \n        setQuery = {setQuery} \n      />\n      <Switch>\n        <Route exact path='/storytimes'>\n          <StorytimeList \n            storytimes = {displayedStorytimes}\n            setCurrentStorytime = {setCurrentStorytime}\n          />\n        </Route>\n        <Route exact path='/activities'>\n          <ActivityList \n            activities = {displayedActivities}\n            setCurrentActivity = {setCurrentActivity}\n          />\n        </Route>\n        <Route exact path='/profile'>\n          <Profile \n            currentUser = {currentUser}\n            setCurrentChild = {setCurrentChild}\n          />\n        </Route>\n        <Route path='/storytimes/:id'>\n          <StorytimeView \n            storytime = {currentStorytime}\n          />\n        </Route>\n        <Route path='/activities/:id'>\n          <ActivityView \n            activity = {currentActivity}\n          />\n        </Route>\n        <div>\n          <button onClick = {handleLogout}>Log out</button>\n          <button onClick = {handleLogin}>Log in</button>\n          {currentUser ? <h1>Welcome, {currentUser.name}</h1> : null}\n        </di